	"JOBS_DETAILS": [
		{
			"NAME": "ProvisionInfraCapabilitiesJob",
			"DESCRIPTION" : "Job to provision INFRA capabilities",
			"ENV_VAR_SCHEDULING_SPEC" : "SCHEDULE_FREQUENCY_FOR_PROVISION_INFRA_CAPABILITIES_JOB",
			"DEFAULT_SCHEDULING_SPEC" : "@every 1m",
			"ENV_VAR_TO_DISABLE_JOB" : "DISABLE_PROVISION_INFRA_CAPABILITIES_JOB"
		},
		{
			"NAME": "GDPRPurgeHistoryJob",
			"DESCRIPTION": "Job to delete subscription/invites history which have exceeded their GDPR retention period (2 years)",
			"ENV_VAR_SCHEDULING_SPEC": "TIME_TO_SCHEDULE_JOBS_AT",
			"DEFAULT_SCHEDULING_SPEC": "0 00 04 * * *",
			"ENV_VAR_TO_DISABLE_JOB": "DISABLE_GDPR_PURGE_HISTORY_JOB"
		},
		{
			"NAME": "IdmCleanupJob",
			"DESCRIPTION" : "Job to clean up expired resources from IdM database",
			"ENV_VAR_SCHEDULING_SPEC" : "TIME_TO_SCHEDULE_JOBS_AT",
			"DEFAULT_SCHEDULING_SPEC" : "@daily",
			"ENV_VAR_TO_DISABLE_JOB" : "DISABLE_IDM_CLEAN_UP_JOB"
		},
		{
			"NAME": "IdmNotifyCertificatesExpiryJob",
			"DESCRIPTION" : "Job for notifying upcoming certificates expiry details.",
			"ENV_VAR_SCHEDULING_SPEC" : "TIME_TO_SCHEDULE_JOBS_AT",
			"DEFAULT_SCHEDULING_SPEC" : "@daily",
			"ENV_VAR_TO_DISABLE_JOB" : "DISABLE_IDM_NOTIFY_CERTIFICATES_EXPIRY_JOB"
		}
	],
